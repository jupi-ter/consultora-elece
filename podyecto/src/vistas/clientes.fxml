<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="326.0" prefWidth="780.0" style="-fx-background-color: orange;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.ClienteController">
    <children>
        <Pane layoutX="14.0" layoutY="14.0" prefHeight="251.0" prefWidth="229.0" style="-fx-border-color: black;">
            <children>
                <TextField fx:id="txtcodigo" disable="true" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="200.0" promptText="ID Empresa" />
                <TextField fx:id="txtnombre" disable="true" layoutX="14.0" layoutY="45.0" prefHeight="25.0" prefWidth="200.0" promptText="Nombre Empresa" />
                <TextField fx:id="txtdireccion" disable="true" layoutX="14.0" layoutY="77.0" prefHeight="25.0" prefWidth="200.0" promptText="Dirección Empresa " />
                <TextField fx:id="txtpropietario" disable="true" layoutX="14.0" layoutY="108.0" prefHeight="25.0" prefWidth="200.0" promptText="Propietario Empresa" />
                <TextField fx:id="txtemail" disable="true" layoutX="14.0" layoutY="140.0" prefHeight="25.0" prefWidth="200.0" promptText="Email de Contacto" />
                <TextField fx:id="txttelcontacto" disable="true" layoutX="14.0" layoutY="175.0" prefHeight="25.0" prefWidth="200.0" promptText="Teléfono de Contacto" />
            </children>
        </Pane>
        <Pane layoutX="243.0" layoutY="14.0" prefHeight="251.0" prefWidth="523.0" style="-fx-border-color: black;">
            <children>
                <TextField fx:id="buscar" layoutX="17.0" layoutY="14.0" onKeyReleased="#buscar" prefHeight="25.0" prefWidth="288.0" promptText="Buscar..." />
                <TableView fx:id="tablaCliente" layoutX="19.0" layoutY="48.0" onMouseClicked="#mostrarDatos" prefHeight="190.0" prefWidth="497.0">
                    <columns>
                        <TableColumn fx:id="columnaIdEmpresa" prefWidth="73.0" text="ID Empresa" />
                        <TableColumn fx:id="columnaNombreEmpresa" prefWidth="104.0" text="Nombre" />
                        <TableColumn fx:id="columnaDirEmpresa" prefWidth="69.0" text="Dirección" />
                        <TableColumn fx:id="columnaPropietarioEmpresa" minWidth="0.0" prefWidth="84.0" text="Propietario" />
                        <TableColumn fx:id="columnaEmailContacto" minWidth="0.0" prefWidth="76.0" text="Email Contacto" />
                        <TableColumn fx:id="columnaTelContacto" prefWidth="90.0" text="Teléfono Contacto" />
                    </columns>
                </TableView>
            </children>
        </Pane>
        <Button fx:id="btnNuevo" layoutX="14.0" layoutY="272.0" onAction="#nuevo" text="Nuevo" />
        <Button fx:id="btnModificar" disable="true" layoutX="75.0" layoutY="272.0" onAction="#modificar" text="Modificar" />
        <Button fx:id="btnEliminar" disable="true" layoutX="154.0" layoutY="272.0" mnemonicParsing="true" onAction="#eliminar" text="Eliminar" />
        <Button fx:id="btnGuardar" disable="true" layoutX="224.0" layoutY="272.0" mnemonicParsing="false" onAction="#guardar" text="Guardar" />
        <Button fx:id="btnCancelar" disable="true" layoutX="294.0" layoutY="272.0" mnemonicParsing="false" onAction="#cancelar" text="Cancelar" />
    </children>
</AnchorPane>
