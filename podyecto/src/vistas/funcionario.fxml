<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="315.0" prefWidth="820.0" style="-fx-background-color: orange;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.FuncionarioController">
    <children>
        <Pane layoutX="14.0" layoutY="14.0" prefHeight="251.0" prefWidth="229.0" style="-fx-border-color: black;">
            <children>
                <TextField fx:id="txtcodigo" disable="true" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="200.0" promptText="ID Funcionario" />
                <TextField fx:id="txtnombre" disable="true" layoutX="15.0" layoutY="45.0" prefHeight="25.0" prefWidth="200.0" promptText="Nombre Funcionario" />
                <TextField fx:id="txtci" disable="true" layoutX="14.0" layoutY="77.0" prefHeight="25.0" prefWidth="200.0" promptText="CI Especialista" />
                <TextField fx:id="txtprofesion" disable="true" layoutX="14.0" layoutY="108.0" prefHeight="25.0" prefWidth="200.0" promptText="Profesión Funcionario" />
                <TextField fx:id="txtespecialidad" disable="true" layoutX="14.0" layoutY="140.0" prefHeight="25.0" prefWidth="200.0" promptText="Especialidad Funcionario" />
                <TextField fx:id="txttelefono" disable="true" layoutX="14.0" layoutY="175.0" prefHeight="25.0" prefWidth="200.0" promptText="Teléfono Especialista" />
                <TextField fx:id="txtemail" disable="true" layoutX="14.0" layoutY="210.0" prefHeight="25.0" prefWidth="200.0" promptText="Email Funcionario" />
            </children>
        </Pane>
        <Pane layoutX="243.0" layoutY="14.0" prefHeight="251.0" prefWidth="523.0" style="-fx-border-color: black;">
            <children>
                <TextField fx:id="buscar" layoutX="17.0" layoutY="14.0" onKeyReleased="#buscar" prefHeight="25.0" prefWidth="288.0" promptText="Buscar..." />
                <TableView fx:id="tablaFuncionario" layoutX="19.0" layoutY="48.0" onMouseClicked="#mostrarDatos" prefHeight="190.0" prefWidth="497.0">
                    <columns>
                        <TableColumn fx:id="columnaIdFuncionario" prefWidth="55.0" text="ID" />
                        <TableColumn fx:id="columnaNombreFuncionario" prefWidth="77.0" text="Nombre" />
                        <TableColumn fx:id="columnaCiEspecialista" prefWidth="50.0" text="CI" />
                        <TableColumn fx:id="columnaProfesionFuncionario" minWidth="0.0" prefWidth="73.0" text="Profesión" />
                        <TableColumn fx:id="columnaEspecialidadFuncionario" minWidth="0.0" prefWidth="96.0" text="Especialidad" />
                        <TableColumn fx:id="columnaTelEspecialista" prefWidth="63.0" text="Teléfono" />
                        <TableColumn fx:id="columnaEmailFuncionario" prefWidth="81.0" text="Email" />
                    </columns>
                </TableView>
            </children>
        </Pane>
        <Button fx:id="btnNuevo" layoutX="14.0" layoutY="272.0" onAction="#nuevo" text="Nuevo" />
        <Button fx:id="btnModificar" disable="true" layoutX="75.0" layoutY="272.0" onAction="#modificar" text="Modificar" />
        <Button fx:id="btnEliminar" disable="true" layoutX="154.0" layoutY="272.0" mnemonicParsing="true" onAction="#eliminar" text="Eliminar" />
        <Button fx:id="btnGuardar" disable="true" layoutX="224.0" layoutY="272.0" mnemonicParsing="false" onAction="#guardar" text="Guardar" />
        <Button fx:id="btnCancelar" disable="true" layoutX="294.0" layoutY="272.0" mnemonicParsing="false" onAction="#cancelar" text="Cancelar" />
    </children>
</AnchorPane>
